package no.hib.dat101.utsyn;

public class a {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		for (int i = 0; i <=10; i++) {

			if (i%2==0) {

				for (int j = (i * 10) + 1; j <= (i * 10) + 10; j++) {
					System.out.print(j);
				}

			}else{
				
				for (int j = (i * 10) + 10; j >= (i * 10) + 1; j--) {
					System.out.print(j);
				}

			}

			System.out.println();
		}
	}

}
